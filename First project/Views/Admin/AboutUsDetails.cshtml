@model Aboutu

@{
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}


<h1>Details</h1>

<div>
    <h4>About us page management</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AboutusImagePath)
        </dt>
        <dd class="col-sm-10">
            <img src=@Url.Content("~/images/" + @Model.AboutusImagePath)
                 width="50" height="50"
                 asp-append-version="true" />
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Header)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Header)
        </dd>
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.AboutusContent)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.AboutusContent)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="AboutUsEdit" asp-route-id="@Model.AboutUs">Edit</a> |
    <a asp-action="AboutUsIndex">Back to List</a>
</div>

